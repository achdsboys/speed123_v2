#include<kipr/botball.h>
void movetiny(void)
{
   
    
    mav(2,250);
    mav(3,250);
    msleep(500);
    ao();
}
void move4in(void)
{
       
    
    
    mav(2,250);
    mav(3,250);
    msleep(1500);
    ao();
} 
void movelittleb(void)
{
    mav(2,-250);
    mav(3,-250);
    msleep(900);
    ao();
    msleep(1000);
} 
void movelittle(void)
{
    mav(2,250);
    mav(3,250);
    msleep(900);
    ao();
    msleep(1000);
} 
void moveccw(void)
{
    //  45 ccw
    mav(2,270);
    mav(3,-270);
    msleep(1650);
    
    ao();
}
    void movecw(void)
{
  mav(2,-270);
    mav(3,270);
    msleep(1650);
        ao();

}
 void moveback(void)
{
      mav(2,-250);
    mav(3,-250);
    msleep(1550);
    ao();
     
 }

 void speed(void)
{
      mav(2,750);
    mav(3,750);
    msleep(1300);
    ao();
     
 }



int main()
{
enable_servos(2);
enable_servos(0);
//if med center2 is on fire
//move servo al the way up
//sc
set_servo_position(2,200);
set_servo_position(2,0);
//go until med center2
speed();speed();speed();speed();speed();
//move servo down
set_servo_position(2,400);
  enable_servos(0);
  //open servo
            set_servo_position (0,1000); 
            set_servo_position (0,1200);
//turn to get other block
moveccw();moveccw();
//go to fire pole
speed();speed();speed();speed();speed();
//turn cw
movecw();movecw();
 
 //move servo down
 enable_servos(2);
                  set_servo_position (2,800);
            set_servo_position (2,950);
             }
//pick up fire man
move4in();move4in();movelittle();
//close servo
set_servo_position (0,800); 
            set_servo_position (0,600);
moveback();moveback();movelittleb();
movecw();movecw();
